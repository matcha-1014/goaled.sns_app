<div class="users-wrapper">
  <div class="users-container">
    <h2 class="page-title">ユーザー一覧</h2>
    <div class="users-box">
      <ul>
        <% @users.each do |user| %>
        <li class="users-text"><%= link_to(user.name, "/users/#{user.id}/show") %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>